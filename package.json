{
  "name": "jst-ai",
  "version": "1.0.0",
  "description": "JSON to TOON (Token Optimized Object Notation) Converter",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --port 8000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "node test.js"
  },
  "keywords": [
    "json",
    "toon",
    "token-optimization",
    "converter"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "papaparse": "^5.4.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.14",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
